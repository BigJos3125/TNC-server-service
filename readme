/*-------SETUP-------*/
pnpm init

pnpm add express

/*-------Run Server-------*/
pnpm add -D nodemon

/*-------Middleware-------*/
pnpm add morgan body-parser cors

/*-------Connect DB-------*/
pnpm add mongoose


pnpm add dotenv


